GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/OAuthConsumer" "${PODS_ROOT}/Headers/Public/TwitterSDK"
LIBRARY_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/OAuthConsumer" "$CONFIGURATION_BUILD_DIR/TwitterSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/OAuthConsumer" -isystem "${PODS_ROOT}/Headers/Public/TwitterSDK"
OTHER_LDFLAGS = $(inherited) -ObjC -l"OAuthConsumer" -l"TwitterSDK"
PODS_ROOT = ${SRCROOT}/Pods
